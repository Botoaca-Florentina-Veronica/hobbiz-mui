{
  "expo": {
    "name": "Hobbiz",
    "slug": "hobbiz-mui-monorepo",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/puzzle_safe.png",
    "scheme": "mobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "verabotoaca",
    "extra": {
      "eas": {
        "projectId": "0c622d46-639a-4bd4-a4d8-ea36c14062df"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.verabotoaca.hobbiz"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/puzzle_safe.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.verabotoaca.hobbiz",
      "softwareKeyboardLayoutMode": "pan",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
